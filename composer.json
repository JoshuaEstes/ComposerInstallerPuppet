{
    "name": "joshuaestes/composer-installer-puppet",
    "description": "This custom installer allows you to manage your puppet modules with composer.",
    "type": "composer-installer",
    "keywords": ["puppet"],
    "homepage": "https://github.com/JoshuaEstes/ComposerInstallerPuppet",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Estes",
            "homepage": "http://joshuaestes.me"
        }
    ],
    "support": {
        "issues": "https://github.com/JoshuaEstes/ComposerInstallerPuppet/issues"
    },
    "autoload": {
        "psr-0": {
            "JoshuaEstes\\Composer\\Installer\\Puppet\\": ""
        }
    },
    "target-dir": "JoshuaEstes/Composer/Installer/Puppet",
    "extra": {
        "class": "JoshuaEstes\\Composer\\Installer\\Puppet\\Installer",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
